@model IEnumerable<Neo4Fredis.Models.DTOs.Sortiranje>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

@*<h1>Get all</h1>*@

<div class="container pt-5">
    <div class="row">
        @*<div class="col-md-6">
        <a class="btn btn-dark" href="/Author/Add">+ Add more</a>
        </div>*@
    </div>
    <div class="row">
        <div class="col-md-6">
            <h3>Aktuelna mesta za izlaske</h3>

            <table class="table table-stripped table-bordered-dark">
                <thead>
                    <tr>
                        @*<th>Sifra mesta</th>*@
                        @* <th>Ime seta</th>*@
                        <th>Naziv</th>
                        <th>Broj zainteresovanih ucesnika</th>
                        @*<th>Predvidjeni Broj ucesnika</th>*@
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            @* <td>@item.siframesta</td>*@
                            <td>@item.imeMesta</td>
                            <td>@item.broj</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>