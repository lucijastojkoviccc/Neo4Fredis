@model IEnumerable<Neo4Fredis.Models.Entities.MestoZaIzlaske>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Moja mesta</h1>

<div class="container pt-5">
    <div class="row">
        <div class="col-md-6">
            <h3>Moja mesta</h3>
                @*<form asp-action="GetMyPlaces" method="GET">*@
                    <table class="table table-stripped table-bordered table-dark">
                        <thead>
                            <tr>
                                <th>Naziv</th>
                                <th>Adresa</th>
                                <th>Opis</th>
                            </tr>
                        </thead>
                      <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.Naziv</td>
                                    <td>@item.Adresa</td>
                                    <td>@item.Opis</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                @*</form>*@
        </div>
    </div>
</div>
